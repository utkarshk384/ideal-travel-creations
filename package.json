{
  "name": "ideal-travel-creations",
  "version": "0.1.0",
  "private": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "scripts": {
    "make-dev-config": "node ./build-scripts/ngrok.js",
    "make-prod-config": "node ./build-scripts/ngrok-build.js",
    "pre-dev": "wait-on -d 1000 file:./next.config.js && next dev",
    "pre-prod": "wait-on -d 1000 file:./next.config.js && next build",
    "dev": "concurrently \"npm run make-dev-config\" \"npm run pre-dev -- -H 192.168.0.105\"",
    "build": "concurrently \"npm run make-prod-config\" \"npm run pre-prod\"",
    "prod-dev": "concurrently \"npm run make-prod-config\" \"npm run next-dev\"",
    "start": "next start  -- -p 4000",
    "generate": "graphql-codegen --config codegen.yml",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@tanem/react-nprogress": "^3.0.58",
    "axios": "^0.21.1",
    "cross-fetch": "^3.1.4",
    "dompurify": "^2.2.7",
    "emailjs-com": "^2.6.4",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3.0.2",
    "graphql": "^15.4.0",
    "gsap": "^3.6.0",
    "imagemin-mozjpeg": "^9.0.0",
    "lodash": "^4.17.20",
    "mapbox-gl": "^2.1.1",
    "next": "10.0.4-canary.4",
    "next-optimized-images": "^2.6.2",
    "next-seo": "^4.23.0",
    "nodemailer": "^6.5.0",
    "nprogress": "^0.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-final-form": "^6.5.2",
    "react-final-form-arrays": "^3.1.3",
    "react-hover-observer": "^2.1.1",
    "react-loader-spinner": "^4.0.0",
    "react-markdown": "^5.0.3",
    "react-multiline-clamp": "^1.0.6",
    "react-transition-group": "^4.4.1",
    "remark-gfm": "^1.0.0",
    "responsive-loader": "^2.3.0",
    "sass": "^1.32.0",
    "sharp": "^0.27.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/clamp-js": "^0.7.2",
    "@types/dompurify": "^2.2.1",
    "@types/lodash": "^4.14.168",
    "@types/mapbox-gl": "^2.1.0",
    "@types/node": "^14.14.20",
    "@types/nodemailer": "^6.4.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-phone-number-input": "^3.0.6",
    "@types/react-transition-group": "^4.4.0",
    "@types/react-truncate": "^2.3.3",
    "@types/smooth-scroll": "^16.1.0",
    "@types/use-prefers-color-scheme": "^0.1.0",
    "@types/uuid": "^8.3.0",
